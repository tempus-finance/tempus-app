{
  "name": "tempus-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "typescript": "^4.6.2"
  },
  "scripts": {
    "start": "lerna run start",
    "build": "lerna run build",
    "test": "lerna run ci:test",
    "bundlewatch": "bundlewatch"
  },
  "devDependencies": {
    "bundlewatch": "^0.3.3",
    "lerna": "^4.0.0"
  },
  "bundlewatch": {
    "files": [
      {
        "path": "./packages/tempus-client_v2/build/static/js/*.js"
      }
    ],
    "ci": {
      "trackBranches": [
        "master",
        "release-*"
      ]
    },
    "normalizeFilenames": "^.+?(\\..+?)\\.\\w+$"
  }
}
