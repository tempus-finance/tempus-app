  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "../../.eslintrc.js"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.stories.{ts,tsx}",
      "!src/**/index.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/reportWebVitals.ts",
      "!src/**/serviceWorker.ts",
      "!**/node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 80,
        "lines": 80,
        "statements": -20
      }
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:update": "react-scripts test -u",
    "ci:test": "CI=true react-scripts test",
    "eject": "react-scripts eject",
    "deploy-stage": "firebase deploy --only hosting:tempus-app-stage --token $FIREBASE_TOKEN",
    "prestart": "./scripts/get-release-version.sh",
    "prebuild": "./scripts/get-release-version.sh build",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "deploy-storybook": "firebase deploy --only hosting:tempus-storybook --token $FIREBASE_TOKEN"
